{
    "version": 2,
    "customTemplates": [
        {
            "name": "eggcerpt-template",
            "title": "Eggcerpt Template",
            "postTypes": [
                "page"
            ]
        }
    ],
    "settings": {
        "appearanceTools": true,
        "color": {
            "palette": [
                {
                    "slug": "foreground",
                    "color": "#000000",
                    "name": "Foreground"
                },
                {
                    "slug": "background",
                    "color": "#FFFFFF",
                    "name": "Background"
                },
                {
                    "slug": "background-secondary",
                    "color": "#F4EEEC",
                    "name": "Primary"
                },
                {
                    "slug": "secondary",
                    "color": "#606060",
                    "name": "Secondary"
                },
                {
                    "slug": "tertiary",
                    "color": "#DC2035",
                    "name": "Tertiary"
                }
            ]
        },
        "custom": {
            "spacing": {
                "small": "max(1.25rem, 5vw)"
            }
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "'Poppins', sans-serif",
                    "name": "Poppins",
                    "slug": "poppins"
                }
            ],
            "fontSizes": [
                {
                    "size": "1.3125rem",
                    "slug": "small"
                },
                {
                    "size": "2.1875rem",
                    "slug": "medium"
                },
                {
                    "size": "3rem",
                    "slug": "large"
                },
                {
                    "size": "3.75rem",
                    "slug": "x-large"
                }
            ]
        },
        "layout": {
            "contentSize": "840px",
            "wideSize": "1100px"
        }
    },
    "styles": {
        "blocks": {
            "core/header": {
                "settings": {
                    "layout": {
                        "wideSize": "100vw"
                    }
                }
            },
            "core/image": {
               
            },
            "core/post-title": {
                
            },
            "core/post-excerpt": {
                "elements": {
                    "typography": {
                        "fontFamily": "var(--wp--preset--font-family--poppins)",
                        "fontSize": "var(--wp--preset--font-size--small)"
                    }
                }
            },
            "core/footer": {
                "margin": 0
            }
        },
        "elements": {
            "h1": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--poppins)",
                    "fontSize": "var(--wp--preset--font-size--x-large)"
                },
                "color": "var(--wp--preset--color--tertiary)"
            },
            "h2": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--poppins)",
                    "fontSize": "var(--wp--preset--font-size--large)",
                    "fontWeight": "600"
                }
            },
            "h3": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--poppins)",
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "400"
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "area": "header",
            "title": "Header"
        },
        {
            "name": "footer",
            "area": "footer",
            "title": "Footer"
        }
    ]
}